<!-- MAIN CONTENT -->
<div class="container">

<!-- search buttons  -->
 <!-- search by subject -->
  <span class="dropdown">
    <button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
      Search by Subject
      <span class="caret"></span>
    </button>
    <ul class="dropdown-menu" aria-labelled by="dropdownMenu1">
    {{#each availSubject}}
      <li><a href="#"> {{subjectName}}</a></li>
    {{/each}}
    </ul>




  </span> <!-- closes first dropdown span -->
 <!-- search by  -->
  <span class="dropdown">
    <button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu2" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
      Search by Name
      <span class="caret"></span>
    </button>
    <ul class="dropdown-menu" aria-labelled by="dropdownMenu1">
    {{#each availTutor}}
      <li><a href="#"> {{firstName}}</a></li>
    {{/each}}
    </ul>
  </span> <!-- closes first dropdown div -->

<!-- search results  -->
  <div class="container searchResults col-md-10">
      <h1> List search results here </h1>
  </div>



</div> <!-- close container div  -->